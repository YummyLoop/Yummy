{
	"format_version": "1.8.0",
	"animations": {
		"open_chest": {
			"loop": "hold_on_last_frame",
			"animation_length": 1,
			"override_previous_animation": true,
			"bones": {
				"Top": {
					"rotation": {
						"0.0": {
							"vector": [0, 0, 0]
						},
						"1.0": {
							"vector": [-90, 0, 0],
							"easing": "easeOutBack"
						}
					}
				}
			},
			"sound_effects": {
				"0.0": {
					"effect": "open.ogg"
				}
			}
		},
		"close_chest": {
			"loop": "hold_on_last_frame",
			"animation_length": 1,
			"override_previous_animation": true,
			"bones": {
				"Top": {
					"rotation": {
						"0.0": {
							"vector": [-90, 0, 0]
						},
						"1.0": {
							"vector": [0, 0, 0],
							"easing": "easeInBounce",
							"easingArgs": [0.1]
						}
					}
				}
			},
			"sound_effects": {
				"0.0": {
					"effect": "close.ogg"
				}
			}
		},
		"open_chest_idle": {
			"loop": true,
			"override_previous_animation": true,
			"bones": {
				"Top": {
					"rotation": {
						"vector": [-90, 0, 0]
					}
				}
			}
		}
	}
}