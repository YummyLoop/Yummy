{
  "schemaVersion": 1,
  "id": "yummy",
  "version": "${version}",
  "name": "Yummy Mod",
  "description": "This is an yummy mod!",
  "authors": [
    "YummyLoop!"
  ],
  "contact": {
    "homepage": "https://github.com/YummyLoop/Yummy",
    "sources": "https://github.com/YummyLoop/Yummy"
  },
  "license": "?",
  "icon": "assets/yummy/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "yummyloop.yummy.fabric.ExampleModFabric"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "yummyloop.yummy.fabric.ExampleModFabric"
      }
    ],
    "modmenu": [
      "yummyloop.yummy.fabric.integration.modmenu.ModMenuIntegration"
    ]
  },
  "mixins": [
    "mixin.examplemod.fabric.json"
  ],
  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": ">=1.16.4",
    "architectury": ">=1.1.55",
    "fabric-language-kotlin": ">=1.4.0",
    "cloth-config2": ">=4.9.5"
  },
  "suggests": {
    "flamingo": "*"
  }
}